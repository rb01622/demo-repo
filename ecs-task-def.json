{
  "family": "rajesh-nodejs-ci-cd-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "nodejs-app",
      "image": "089204452167.dkr.ecr.us-east-1.amazonaws.com/nodejs-ci-cd-app:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
	
